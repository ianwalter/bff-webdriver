{
  "name": "@ianwalter/bff-webdriver",
  "description": "A bff plugin to enable WebDriver-based testing",
  "version": "1.2.0",
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "homepage": "https://github.com/ianwalter/bff-webdriver#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js tests.js",
    "test": "bff",
    "postinstall": "node postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/bff-webdriver.git"
  },
  "bugs": {
    "url": "https://github.com/ianwalter/bff-webdriver/issues"
  },
  "keywords": [
    "bff",
    "webdriver",
    "selenium",
    "test",
    "testing",
    "automated",
    "standalone",
    "browserstack"
  ],
  "files": [
    "index.js",
    "postinstall.js"
  ],
  "dependencies": {
    "@ianwalter/print": "^2.3.0",
    "browserstack-local": "^1.3.7",
    "got": "^9.6.0",
    "selenium-standalone": "^6.16.0",
    "webdriverio": "^5.7.14"
  },
  "devDependencies": {
    "@ianwalter/bff": "^4.0.1",
    "@ianwalter/eslint-config": "^1.1.2",
    "@ianwalter/test-server": "^1.3.0",
    "dotenv": "^7.0.0",
    "np": "^5.0.0"
  },
  "bff": {
    "logLevel": "debug",
    "plugins": [
      "config",
      "index"
    ],
    "webdriver": {
      "capabilities": {
        "browserName": "firefox",
        "bstack:options": {
          "projectName": "bff-webdriver",
          "timezone": "EST"
        }
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ianwalter"
    ]
  }
}
