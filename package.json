{
  "name": "@ianwalter/bff-webdriver",
  "description": "A bff plugin to enable WebDriver-based testing",
  "version": "5.6.0",
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "homepage": "https://github.com/ianwalter/bff-webdriver#readme",
  "license": "SEE LICENSE IN LICENSE",
  "bin": {
    "bff-webdriver": "cli.js"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "bff",
    "bff-webdriver": "./cli.js",
    "postinstall": "./cli.js setup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/bff-webdriver.git"
  },
  "bugs": {
    "url": "https://github.com/ianwalter/bff-webdriver/issues"
  },
  "keywords": [
    "bff",
    "webdriver",
    "selenium",
    "test",
    "testing",
    "automated",
    "standalone",
    "browserstack"
  ],
  "files": [
    "index.js",
    "cleanup.js",
    "cli.js",
    "integrations"
  ],
  "dependencies": {
    "@ianwalter/bsl": "^2.1.1",
    "@ianwalter/cli": "^4.1.0",
    "@ianwalter/print": "^5.3.0",
    "dotenv": "^8.2.0",
    "execa": "^4.0.0",
    "got": "^10.6.0",
    "p-settle": "^3.1.0",
    "selenium-standalone": "^6.17.0",
    "webdriverio": "^5.18.7"
  },
  "devDependencies": {
    "@ianwalter/bff": "^9.8.2",
    "@ianwalter/eslint-config": "^4.1.0",
    "@ianwalter/release": "^5.0.0",
    "@ianwalter/renovate-config": "^1.3.0",
    "@ianwalter/test-server": "^2.1.0"
  },
  "bff": {
    "logLevel": "debug",
    "plugins": [
      "config",
      "index"
    ],
    "webdriver": {
      "capabilities": {
        "browserName": "firefox"
      }
    }
  },
  "renovate": {
    "extends": [
      "@ianwalter"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ianwalter"
    ]
  }
}
